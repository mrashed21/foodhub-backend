model Category {
    id             String   @id @default(uuid())
    name           String   @unique
    slug           String   @unique
    category_image String?
    isActive       Boolean  @default(true)
    published_by   String
    createdAt      DateTime @default(now())
    updatedAt      DateTime @updatedAt
    user           User     @relation(fields: [published_by], references: [id])

    @@map("category")
}
