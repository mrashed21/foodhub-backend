model Category {
    id             String   @id @default(uuid())
    name           String   @unique
    slug           String   @unique
    categoryImage String?
    isActive       Boolean  @default(true)
    publishedBy   String
    createdAt      DateTime @default(now())
    updatedAt      DateTime @updatedAt
    user           User     @relation(fields: [publishedBy], references: [id])
    menus          Menu[]

    @@map("category")
}
